{
  "name": "optimistdigital/laravel-console-over-http",
  "description": "Run console commands over HTTP connection for Laravel applications.",
  "keywords": [
    "laravel",
    "console",
    "optimistdigital"
  ],
  "license": "MIT",
  "require": {
    "laravel/framework": "^6.0 || ^7.0 || ^8.0",
    "symfony/process": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\LaravelConsoleOverHttp\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\LaravelConsoleOverHttp\\ServiceProvider"
      ],
      "aliases": {
        "ConsoleOverHttp": "OptimistDigital\\LaravelConsoleOverHttp\\Facade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
